{% set interfaces = importInterfaces(operations, ['subscribe']) -%}
{% if interfaces %}import { {% for interface in interfaces -%}{{ interface }},{%- endfor %} } from '../interfaces';{% endif %}

export interface {{ ucFirst(operation_name) }}DomainInterface {
  {% for op in operations -%}
  {% for method, channel in op.channel -%}
  {% if method === 'subscribe' %}
  {{ channel.operationId }} (payload: {{ channel['x-response-definitions'] }} ): Promise<void>;
  {% endif %}
  {% endfor -%}
  {% endfor -%}
}
